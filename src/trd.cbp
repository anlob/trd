<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="trd" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Debug">
				<Option output="bin/Debug/trd" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Debug/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-g" />
				</Compiler>
				<Linker>
					<Add option="-Wl,-rpath,$(PROJECT_DIR)../libcpp-core/src/bin/Debug" />
					<Add option="-Wl,-rpath,$(PROJECT_DIR)../libtrapi/src/bin/Debug" />
					<Add directory="../libcpp-core/src/bin/Debug" />
					<Add directory="../libtrapi/src/bin/Debug" />
				</Linker>
				<ExtraCommands>
					<Add before="$(PROJECT_DIR)/../libcpp-core/tools/mergeworktree.sh" />
				</ExtraCommands>
			</Target>
			<Target title="Release">
				<Option output="bin/Release/trd" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Release/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-O2" />
				</Compiler>
				<Linker>
					<Add option="-s" />
					<Add directory="../libcpp-core/src/bin/Release" />
					<Add directory="../libtrapi/src/bin/Release" />
				</Linker>
				<ExtraCommands>
					<Add before="$(PROJECT_DIR)/../libcpp-core/tools/mergeworktree.sh -c" />
				</ExtraCommands>
			</Target>
		</Build>
		<Compiler>
			<Add option="-Wall" />
			<Add option="-std=c++11" />
			<Add option="-fexceptions" />
		</Compiler>
		<Linker>
			<Add option="-lssl" />
			<Add option="-ljsoncpp" />
			<Add library="libtrapi" />
			<Add library="libcpp-core" />
		</Linker>
		<Unit filename="conf.h" />
		<Unit filename="main.cpp" />
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
			<lib_finder disable_auto="1">
				<lib name="libssl" />
				<lib name="jsoncpp" />
			</lib_finder>
		</Extensions>
	</Project>
</CodeBlocks_project_file>
